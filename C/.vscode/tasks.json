{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "binary_search",
      "type": "shell",
      "command": "/usr/bin/g++-9",
      "args": [
        "-Wall",
        "-Wextra",
        "-pedantic",
        "-fno-omit-frame-pointer",
        "-g",
        "${file}",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}",
        "-DALGO_WITH_SQRT"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "reveal": "silent"
      },
      "problemMatcher": "$msCompile"
    },
    {
      "type": "shell",
      "label": "g++ build active file",
      "command": "/usr/bin/g++",
      "args": [
        "-g",
        "${file}",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}"
      ],
      "options": {
        "cwd": "/usr/bin"
      }
    }
  ]
}